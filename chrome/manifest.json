{
    "name": "ShockrTools",
    "version": "4.5.3",
    "manifest_version": 2,
    "description": "Alliance tools for Tiberium Alliances",
    "homepage_url": "https://c.ac.nz/",
    "permissions": [
        "*://*.alliances.commandandconquer.com/*/index.aspx*"
    ],
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "content_scripts": [{
        "matches": [
            "*://*.alliances.commandandconquer.com/*/index.aspx*"
        ],
        "js": [
            "shockrtools.user.js"
        ]
    }]
}